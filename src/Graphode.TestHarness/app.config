<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="LoggingMode" value="All"/><!--values are All,Application-->
    <add key="ApplicationsLogPath" value="applications.log"/>
    <add key="MainLogPath" value="main.log"/>
    <add key="DataLoadLogPath" value="dataload.log"/>
    <add key="ApplicationPatterns" value="WebUI$,WebService$,WebAPI$,WindowsService$,ConsoleUI$"/>
    <add key="CompanyAssembliesPattern" value="^BadgersInc"/>
    <add key="LocalNeo4jUrl" value="http://neo4j:12345@localhost:7474/db/data"/>
